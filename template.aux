\@writefile{lof}{\contentsline {figure}{\numberline {6}{\ignorespaces Pressure--latitude distributions of relative humidity and cloud fraction of EXP (a,d), CNTL (b,e), EXP-CNTL (c,f).}}{19}{figure.6}}
